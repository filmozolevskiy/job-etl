# Job-ETL Environment Variables
# Copy this file to .env and fill in your actual values
# The .env file is gitignored and will not be committed

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=CHANGE_THIS_TO_A_STRONG_PASSWORD

# Airflow Configuration
AIRFLOW_ADMIN_PASSWORD=CHANGE_THIS_TO_A_STRONG_PASSWORD
AIRFLOW_POSTGRES_PASSWORD=secure_airflow_postgres_password_123
AIRFLOW__CORE__FERNET_KEY=CHANGE_THIS_GENERATE_WITH_openssl_rand_base64_32
AIRFLOW__WEBSERVER__SECRET_KEY=CHANGE_THIS_GENERATE_WITH_openssl_rand_base64_32

# Database Configuration (optional - already using Docker secrets)
# POSTGRES_DB=job_etl
# POSTGRES_USER=job_etl_user
# POSTGRES_PORT=5432

# Timezone
TIMEZONE=America/Toronto
